  <body>

    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="#">Offers</a>
        </div>
      </div>
    </nav>

    <div class="jumbotron">
      <div class="container">
        <form>
          <div class="form-group col-xs-8">
            <label for="uid">Uid</label>
            <input type="text" class="form-control" id="uid" name="uid" placeholder="player1">
          </div>

          <div class="form-group col-xs-8">
            <label for="page">Page</label>
            <input type="text" class="form-control" id="page" name="page" placeholder="1">
          </div>

          <div class="form-group col-xs-8">
            <label for="pub0">pub0</label>
            <input type="text" class="form-control" id="pub0" name="pub0" placeholder="campaign2">
          </div>

          <div class="form-group">
            <div class="col-sm-10">
              <button type="button" class="btn btn-default" onclick="loadOffers();">Load offers</button>
            </div>
          </div>
        </form>
      </div>
    </div>

    <div class="container" id="offers">
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://getbootstrap.com/dist/js/bootstrap.min.js"></script>
    <script src="/js/app.js"></script>
